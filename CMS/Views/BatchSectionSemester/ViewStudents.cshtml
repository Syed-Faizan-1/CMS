@model CMS.ViewModels.StudentGroupViewModel

<h1>Students in @Model.SectionName - @Model.BatchName, @Model.CampusName</h1>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Roll No</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model.Students)
        {
            <tr>
                <td>@student.FirstName</td>
                <td>@student.LastName</td>
                <td>@student.RollNo</td>
            </tr>
        }
    </tbody>
</table>
